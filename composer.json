{
    "name": "kpacha/sifo-plugin-installer",
    "description": "Simple composer plugin for installing sifo components",
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
    {
        "name": "Kpacha",
        "email": "kpacha666@gmail.com"
    }
    ],
    "require": {
        "composer-plugin-api": "~1.0"
    },
    "require-dev": {
        "composer/composer": "1.0.*@dev"
    },
    "extra": {
        "class": "Sifo\\Composer\\Installer"
    },
    "autoload": {
        "psr-4": {
            "Sifo\\Composer\\": "src/"
        }
    }
}
